{# version 1.00, date 14/09/2016, auteur Florian Leriche #}
{% extends "UserBundle::layout.html.twig" %}

{% block title %}

    Profil - {{ parent() }}

{% endblock %}

{% block body %}

    <script src="{{ asset('vendor/pdfmake/build/pdfmake.min.js') }}"></script>
    <script src="{{ asset('vendor/pdfmake/build/vfs_fonts.js') }}"></script>
    <script>
        function imprimer(){
            var docDefinition = {
                content: [
                    {
                        image: 'data:image/png;base64,',
                        width: 75
                    },
                    {text: "Fiche de Benevole\n\n", style: ['header']},
                    {text: "Nom: \n\n", style: ['title']},
                    {text: "{{ benevole.nom | capitalize }}\n\n", style: ['body']},
                    {text: "Prenom: \n\n", style: ['title']},
                    {text: "{{ benevole.prenom | capitalize}}\n\n", style: ['body']},
                    {text: "Email:\n\n", style: ['title']},
                    {text: "{{ benevole.email}}\n\n", style: ['body']},
                    {text: "Téléphone fixe: \n\n", style: ['title']},
                    {text: "{% if benevole.telFixe %}{{ benevole.telFixe}}{% else %} - {% endif %}\n\n", style: ['body']},
                    {text: "Téléphone portable: \n\n", style: ['title']},
                    {text: "{% if benevole.telPortable %}{{ benevole.telPortable}}{% else %} - {% endif %}\n\n", style: ['body']},
                    {text: "Adresse: \n\n", style: ['title']},
                    {text: "{{ benevole.adresse.adresse}}, {{ benevole.adresse.codePostal.code }} {{ benevole.adresse.ville.nom }}\n\n", style: ['body']},
                    {text: "Comité(s) de rattachement: \n\n", style: ['title']},
                    {text: "Seine-Maritime\n\n", style: ['body']},
                    {text: "Responsabilités: \n\n", style: ['title']},
                    {% if (benevole.responsabiliteActivite is not empty) %}
                            {% if ('plaidoyers' in benevole.responsabiliteActivite ) %}
                    {text: "Action éducative\n\n", style: ['body']},
                            {% endif %}
                            {% if ('frimousses' in benevole.responsabiliteActivite) %}
                    {text: "Frimousse\n\n", style: ['body']},
                            {% endif %}
                            {% if ('actions_ponctuelles' in benevole.responsabiliteActivite) %}
                    {text: "Actions ponctuelles\n\n", style: ['body']},
                            {% endif %}
                            {% if ('projets' in benevole.responsabiliteActivite) %}
                    {text: "Projets\n\n", style: ['body']},
                            {% endif %}
                            {% if ('admin_comite' in benevole.responsabiliteActivite) %}
                    {text: " Administrateur du comité\n\n", style: ['body']},
                            {% endif %}
                             {% if ('admin_region' in benevole.responsabiliteActivite) %}
                    {text: "Administrateur de la région\n\n", style: ['body']},
                                {% endif %}
                            {% else %}
                    {text: "Aucune \n\n", style: ['body']},
                            {% endif %}
                    {text: "Domaines d'activités: \n\n", style: ['title']},
                    {% if (benevole.activitesPotentielles is not empty) %}
                    {% if ('plaidoyers' in benevole.activitesPotentielles ) %}
                    {text: "Actions éducatives", style: ['body']},
                        {% endif %}
                        {% if ('frimousses' in benevole.activitesPotentielles) %}
                    {text: "Frimousse", style: ['body']},
                        {% endif %}
                        {% if ('actions_ponctuelles' in benevole.activitesPotentielles) %}
                    {text: "Actions ponctuelles", style: ['body']},
                        {% endif %}
                        {% if ('projets' in benevole.activitesPotentielles) %}
                    {text: "Projets", style: ['body']},
                        {% endif %}
                        {% if ('autre' in benevole.activitesPotentielles) %}
                    {text: " Autre", style: ['body']},
                        {% endif %}
                    {% else %}
                    {text: "Aucun", style: ['body']},
                    {% endif %}


                ],
                    styles: {
                        header: {
                            bold:true,
                                    color:'#09F',
                                    fontSize: 16,
                                    alignment: 'center'
                        },
                        body: {
                            fontSize: 9,
                                    alignment: 'left',
                                    margin: [20, 0, 40, 0],
                        },
                        title: {
                            fontSize: 9,
                                    bold: true,
                                    alignment: 'left'
                        }
                    }
            };
            pdfMake.createPdf(docDefinition).download("Fiche-Intervention-"+ {{ benevole.id }} +".pdf");
        }
    </script>
    <style>

        .alert-success{
            margin-top:30px;
        }
    </style>


    <div class="row">
        <div class="btn-group" style="float: right; margin-top:20px;">
            <button type="button" class="imprimente" style="margin-top: 40px; margin-right: 80px;" onclick="imprimer()" ><span class="glyphicon glyphicon-print"></span></button>
        </div>
    </div>
        <div class="row">
            <section class="jumbotron col-sm-12 col-lg-8 col-lg-offset-2">
                <div class="infoPrincip">
                    <div class="row">
                        <h2 class="col-lg-9 titre" style="margin-top: 40px;"> {{ benevole.nom | upper }}
                            &nbsp {{ benevole.prenom | capitalize }}</h2>
                        <img src="{{ asset('img/logo-gris.png') }}" class="logoGris" alt="Logo gris">
                    </div>
                    <hr>
                    <div class="row">
                        <dl class="dl-horizontal">
                            <div class="col-sm-4 col-xs-12">
                                <dt>
                                    <span class="glyphicon glyphicon-envelope"></span> Email :
                                </dt>
                            </div>
                            <div class="col-sm-8">
                                {% if (benevole.email == "" or benevole.email == NULL) %}
                                    {{ "Non renseigné" }}
                                {% else %}
                                    <a href="mailto:{{ benevole.email }}">{{ benevole.email }}</a>
                                {% endif %}
                            </div>
                            <div class="col-sm-4 col-xs-12">
                                <dt>
                                    <span class="glyphicon glyphicon-phone-alt"></span> Téléphone fixe :
                                </dt>
                            </div>
                            <div class="col-sm-8">
                                {% if (benevole.telFixe == "" or benevole.telFixe == NULL) %}
                                    {{ "Non renseigné" }}
                                {% else %}
                                    {{ benevole.telFixe }}
                                {% endif %}
                            </div>
                            <div class="col-sm-4 col-xs-12">
                                <dt>
                                    <span class="glyphicon glyphicon-phone"></span> Téléphone portable :
                                </dt>
                                </dt>
                            </div>
                            <div class="col-sm-8">
                                {% if (benevole.telPortable == "" or benevole.telPortable == NULL) %}
                                    {{ "Non renseigné" }}
                                {% else %}
                                    {{ benevole.telPortable }}
                                {% endif %}
                            </div>
                            <div class="col-sm-4 col-xs-12">
                                <dl>
                                    <dt>
                                        <span class="glyphicon glyphicon-home"></span> Adresse :</td>
                                    </dt>
                                </dl>

                            </div>
                            <div class="col-sm-8">
                                {{ benevole.adresse.getAdresse() }},
                                {% if benevole.adresse.complement is not empty %}
                                    {{ benevole.adresse.complement }},
                                {% endif %}
                                {{ benevole.adresse.codePostal.code }}
                                {{ benevole.adresse.ville.nom }}
                            </div>
                        </dl>

                    </div>
                </div>


            </section>
        </div>
        <div class="row">
            <div class="buttons">
                <a href="{{ path("user_admin_profil_edit", {'username': benevole.username}) }}" class="btn btn-primary"><span
                            class="glyphicon glyphicon-pencil"></span> &nbsp Modifier </a>
                <a href="{{ path("user_admin_delete", {'username': benevole.username}) }}" class="btn btn-danger"
                   onclick="return confirm('Souhaitez-vous vraiment supprimer le profil de {{ benevole.prenom }} {{ benevole.nom }}?')"><span
                            class="glyphicon glyphicon-trash"></span> &nbsp Supprimer </a>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 col-sm-12 col-lg-8 col-lg-offset-2 infoContainer">
                <div class="col-sm-4 containerOne">
                    <div class="circle circleOne">
                        <img src="{{ asset('img/location.png') }}" alt="Location">
                    </div>
                    <h3>Comité(s) de rattachement : </h3>
                    <div class="row">
                        {#{% if benevole.comite is not empty %}#}
                        <ul class="list-group col-lg-offset-1 col-sm-7 ">
                            {#{% for comite in benevole.comite %}#}
                            <li class="list-group-item">
                                {#{% for dep in c.departement %}#}
                                {#<li> {{ dep.nom }} </li>#}
                                {#{% endfor %}#}
                                Seine-Maritime
                            </li>
                            {#{% endfor %}#}
                        </ul>
                        {#{% else %}#}
                        {#<div class="col-lg-offset-1 col-sm-7">#}
                        {#Aucun#}
                        {#</div>#}
                        {#{% endif %}#}
                    </div>
                </div>
                <div class="col-sm-4 containerTwo">
                    <div class="circle circleTwo">
                        <img src="{{ asset('img/briefcase.png') }}" alt="briefcase">
                    </div>

                    <h3>Responsabilités : </h3>
                    {% if (benevole.responsabiliteActivite is not empty) %}
                        <ul class="list-group col-lg-offset-1 col-sm-7 ">
                            {% if ('plaidoyers' in benevole.responsabiliteActivite ) %}
                                <li class="list-group-item">
                                    Action éducative
                                </li>
                            {% endif %}
                            {% if ('frimousses' in benevole.responsabiliteActivite) %}
                                <li class="list-group-item">
                                    Frimousse
                                </li>
                            {% endif %}
                            {% if ('actions_ponctuelles' in benevole.responsabiliteActivite) %}
                                <li class="list-group-item">
                                    Actions ponctuelles
                                </li>
                            {% endif %}
                            {% if ('projets' in benevole.responsabiliteActivite) %}
                                <li class="list-group-item">
                                    Projets
                                </li>
                            {% endif %}
                            {% if ('admin_comite' in benevole.responsabiliteActivite) %}
                                <li class="list-group-item">
                                    Administrateur du comité
                                </li>
                            {% endif %}
                            {% if ('admin_region' in benevole.responsabiliteActivite) %}
                                <li class="list-group-item">
                                    Administrateur de la région
                                </li>
                            {% endif %}
                        </ul>
                    {% else %}
                        <div class="col-lg-offset-1 ">
                            Aucune
                        </div>

                    {% endif %}


                </div>
                <div class="col-sm-4 containerThree">
                    <div class="circle circleThree">
                        <img src="{{ asset('img/help.png') }}" alt="help">
                    </div>
                    <h3>Domaines d'activités : </h3>
                    <div class="row">
                        {% if (benevole.activitesPotentielles is not empty) %}
                            <ul class="list-group col-lg-offset-1 col-sm-7 ">
                                {% if ('plaidoyers' in benevole.activitesPotentielles ) %}
                                    <li class="list-group-item">
                                        Actions éducatives
                                    </li>
                                {% endif %}
                                {% if ('frimousses' in benevole.activitesPotentielles) %}
                                    <li class="list-group-item">
                                        Frimousse
                                    </li>
                                {% endif %}
                                {% if ('actions_ponctuelles' in benevole.activitesPotentielles) %}
                                    <li class="list-group-item">
                                        Actions ponctuelles
                                    </li>
                                {% endif %}
                                {% if ('projets' in benevole.activitesPotentielles) %}
                                    <li class="list-group-item">
                                        Projets
                                    </li>
                                {% endif %}
                                {% if ('autre' in benevole.activitesPotentielles) %}
                                    <li class="list-group-item">
                                        Autre
                                    </li>
                                {% endif %}
                            </ul>
                        {% else %}
                            <div class="col-lg-offset-1 col-sm-7">
                                Aucun
                            </div>
                        {% endif %}
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-4 col-lg-offset-4 col-sm-6 col-sm-offset-3 infoContainer">
                <div class="col-sm-6 containerFour">
                    <div class="circle circleFour">
                        <img src="{{ asset('img/speech-bubbles.png') }}" alt="bubbles">
                    </div>
                    <h3>Interventions : </h3>
                    <div class="row">
                        {% if listeInterventions is not empty %}
                            <div class="scrollspy-example col-lg-offset-1 col-sm-11" data-offset="0"
                                 data-target="#navbar-example" data-spy="scroll" style="
                                    overflow: auto;
                                    position: relative;
                                    border:1px solid #cccccc;
                                    padding:10px;">
                                {% for intervention in listeInterventions %}
                                    <a href="{{ path('intervention_view', {'id': intervention.id}) }}">
                                        {% if intervention.dateIntervention %}
                                            {{ intervention.dateIntervention|date('Y-m-d') }} -
                                        {% endif %}
                                        {{ intervention.etablissement.adresse.ville.nom }}
                                        {% if intervention.plaidoyer %}
                                            - Action éducative
                                        {% elseif intervention.frimousse %}
                                            - Frimousse
                                        {% else %}
                                            - Autre intervention
                                        {% endif %}
                                        </br>
                                    </a>
                                {% endfor %}
                            </div>
                        {% else %}
                            <div class="col-lg-offset-1 col-sm-7">
                                Aucune
                            </div>
                        {% endif %}
                    </div>
                    <h3>Projets en cours : </h3>
                    <div class="row">
                        {% if benevole.projet is not empty %}
                        <div class="scrollspy-example col-lg-offset-1 col-sm-11" data-offset="0"
                             data-target="#navbar-example" data-spy="scroll" style="
                                    overflow: auto;
                                    position: relative;
                                    border:1px solid #cccccc;
                                    padding:10px;">
                            {% for projet in benevole.projet %}
                                <a href="">
                                    {{ projet.getNom() }}
                                    </br>
                                </a>
                            {% endfor %}
                            {% else %}
                                <div class="col-lg-offset-1 col-sm-7">
                                    Aucun
                                </div>
                            {% endif %}
                        </div>
                    </div>
                </div>
            </div>

        </div>

    </div>
{% endblock %}
